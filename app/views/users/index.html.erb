<h1>Users</h1>
<%= form_tag(users_path(format: "xlsx"), {method: :get, class: "form-inline", role: "form"}) do %>
  <div class="form-group">
    <%=label_tag :transaction_start_date%>
    <%=text_field_tag :start_date,'', class: "form-control input-sm datepicker" %>
  </div><!-- form group [search] -->
  <div class="form-group">
    <%=label_tag :transaction_end_date%>
    <%=text_field_tag :end_date,'', class: "form-control input-sm datepicker" %>
  </div><!-- form group [search] -->
  <div class="form-group">
    <%=button_tag class: "btn btn-default filter-col" do%>
      <span class="glyphicon glyphicon-search"></span> Download Excel 
    <% end %>
  </div>

  <table class="table">
    <thead>
      <tr>
        <th>Select</th>
        <th>User Name</th>
        <th>Mobile No</th>
        <th>Email Id</th>
        <th>Last Sign In</th>
        <th>Account Open At</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%=render @users%>
    </tbody>
  </table>
<% end %>
<%= will_paginate @users, renderer: BootstrapPagination::Rails %>